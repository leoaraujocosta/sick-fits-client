{
  "env": {
    "development": {
      "presets": ["next/babel", "@babel/preset-react", "@babel/preset-env"],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ],
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "production": {
      "presets": ["next/babel", "@babel/preset-react", "@babel/preset-env"],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ],
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ],
        "@babel/preset-react",
        "@babel/preset-env"
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ],
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
